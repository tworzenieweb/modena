#: admin/about.php:44
#@ nggallery
msgid "Donate the work via paypal"
msgstr "Donirajte preko Paypala"

#: admin/about.php:57
#@ nggallery
msgid "No doubt a very useful and easy motivation :-)"
msgstr "Bez sumnje veoma laka i korisna motivacija :-)"

